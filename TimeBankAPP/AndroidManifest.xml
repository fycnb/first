<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.timebankapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".First.FirstActivity" />
        <activity android:name=".News.NewsActivity" />
        <activity android:name=".Convert.DHActivity" />
        <activity android:name=".My.MYActivity" />
        <activity android:name=".Convert.CashActivity" />
        <activity android:name=".Convert.ServiceActivity" />
        <activity android:name=".Convert.GoodsActivity" />
        <activity
            android:name=".Convert.MoneyDetailsActivity"
            android:label="@string/title_activity_money_details" >
        </activity>
        <activity
            android:name=".PublishActivity"
            android:label="@string/title_activity_publish" >
        </activity>
    </application>

</manifest>
